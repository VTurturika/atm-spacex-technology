               <children>
                  <Button fx:id="withdrawCash" mnemonicParsing="false" prefHeight="78.0" prefWidth="122.0" text="Withdraw Cash" />
                  <Button fx:id="addCash" mnemonicParsing="false" prefHeight="78.0" prefWidth="122.0" text="Add Cash" />

